<mvc:View
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
	controllerName="sap.ui.core.tutorial.odatav4.controller.App"
	displayBlock="true"
>
		<SplitApp id="SplitAppDemo" initialDetail="detail" initialMaster="queryPage" orientationChange="onOrientationChange">
			<detailPages>
				<mvc:XMLView viewName="sap.ui.core.tutorial.odatav4.view.Query" />
			</detailPages>
			<masterPages>
				<Page id="master" title="Navigation" backgroundStyle="solid">
					<content>
						<List itemPress="onListItemPress">
						<items>
							<StandardListItem title="Query" type="Active" custom:to="queryPage"/>
							<StandardListItem title="Wow" type="Active" custom:to="wowPage"/>
						</items>
					</List>
					</content>
				</Page>
			</masterPages>
		</SplitApp>
</mvc:View>
